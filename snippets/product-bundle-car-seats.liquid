
<div class="product-bundle-car-seats" data-is-bundle-product="{{ is_bundle_product }}">
    <span>{{ 'products.product.select_car_seat' | t }}:</span>
    {% assign alternative_bundle_products = product.metafields.cake.alternative_car_seat_bundles.value %}
    <ul class="product-bundle-car-seats__list">
        {% for item in alternative_bundle_products.products.value %}
           {% render 'bundle-item', item: item %}
        {% endfor %}
    </ul>
</div>