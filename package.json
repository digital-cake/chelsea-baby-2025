{
  "devDependencies": {
    "@by-association-only/vite-plugin-shopify-clean": "^2.2.0",
    "@shopify/prettier-plugin-liquid": "^1.9.0",
    "lightningcss": "^1.29.3",
    "npm-run-all": "^4.1.5",
    "postcss-nesting": "^13.0.1",
    "sass-embedded": "^1.86.0",
    "vite": "^6.2.2",
    "vite-plugin-page-reload": "^0.2.1",
    "vite-plugin-shopify": "^3.2.0"
  },
  "type": "module",
  "scripts": {
    "dev": "run-p -sr \"shopify:dev -- {@}\" \"vite:dev\" --",
    "deploy": "run-s \"vite:build\" \"shopify:push -- {@}\" -- --environment=staging",
    "shopify:dev": "shopify theme dev --environment=staging --theme-editor-sync",
    "shopify:push": "shopify theme push",
    "vite:dev": "vite",
    "vite:build": "vite build",
    "deploy:staging": "npm run deploy -- --environment=staging",
    "deploy:production": "npm run deploy -- --environment=production"
  },
  "dependencies": {
    "@fancyapps/ui": "^6.1.7",
    "axios": "^1.9.0",
    "moment": "^2.30.1",
    "swiper": "^11.2.6"
  }
}
